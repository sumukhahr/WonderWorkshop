    with open('fruits.txt', 'w') as f2:
        for i in range(3):
            if len(d2[i]) > 1:
                f2.write("%s %s\n" %(d2[i][1][0],d2[i][0]))
                f2.write("%s %s\n" % (d2[i][1][1], d2[i][0]))
            else:
                f2.write("%s %s\n" %(d2[i][1]), d2[i][0])

            #f2.write("%s %s\n" % (d2[i][1], d2[i][0]))